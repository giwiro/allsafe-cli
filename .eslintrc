{
  "parser": "babel-eslint",
  "extends": [
    "plugin:flowtype/recommended"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "plugins": [
    "flowtype"
  ],
  "rules": {
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0
  },
  "settings": {
    "flowtype": {
      "onlyFilesWithFlowAnnotation": true
    }
  }
}